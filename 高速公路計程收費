#include <iostream>
using namespace std;
int main(){
	int x,y; //x為進入高速公路時的里程數 y為出去時 
	cin >> x >> y;
	int a,b,c; //a是21~200 b是201~400 c是差值 
	c=y-x;
	if(c<=20){ //參數修改處 
	cout << "0";
	}
	if(c>21 &&c<=200){ //參數修改處 
	a=c-20;
	cout << a*4; //參數修改處 
	}
	if(c>=201){ //參數修改處 
	a=180;
	b=c-200;
	cout << a*4+b*3; //參數修改處 
	}
    return 0;
}
