a=
return a;
}*/

int main (){
	srand(time(NULL));
	int b=0;
	int x[7];
	for(int i=0;i<7;i++){
//	b=(rand()%42)+1;
	x[i]=b;
	}
	for(int i=0;i<7;i++){
		for(int j=0;j<7;j++){
		if(i==j) continue;
		if(x[i]==x[j]){
//		b=(rand()%42)+1;
		x[i]=b;
		}
		}
	}
	for(int i=0;i<7;i++) cout << x[i] << " ";
	return 0;
}
